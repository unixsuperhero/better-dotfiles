echo
echo 'in file: /Users/macbookpro/dotfiles/zsh/prezto/zshrc'
echo 'in file: /Users/macbookpro/dotfiles/zsh/prezto/zshrc'
echo 'in file: /Users/macbookpro/dotfiles/zsh/prezto/zshrc'
echo "{ZDOTDIR:-HOME}/init.zsh: ${ZDOTDIR:-$HOME}/init.zsh"
echo

# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/init.zsh" ]]; then
  echo 'sourcing: zsh/prezto/init.zsh'
  source "${ZDOTDIR:-$HOME}/init.zsh"
fi

# Customize to your needs...

cat <<"END" >/dev/null
