-rw-r--r--  1 macbookpro  staff  60 Mar 13 14:59 /Users/macbookpro/.zlogin
-rw-r--r--  1 macbookpro  staff  61 Mar 13 14:56 /Users/macbookpro/.zlogout
-rw-r--r--  1 macbookpro  staff  62 Mar 13 14:55 /Users/macbookpro/.zprofile
-rw-r--r--  1 macbookpro  staff  60 Mar 13 14:55 /Users/macbookpro/.zshenv
-rw-r--r--  1 macbookpro  staff  59 Mar 13 14:55 /Users/macbookpro/.zshrc
