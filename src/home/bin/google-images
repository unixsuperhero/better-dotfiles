#!/Users/macbookpro/.rbenv/versions/2.1.0/bin/ruby

require 'uri'

q = ARGV.join(' ')
encoded_q = URI.encode(q)

`open "http://google.com/search?tbm=isch&q=#{encoded_q}"`

__END__

