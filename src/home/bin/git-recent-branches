#!/usr/bin/env ruby

require 'awesome_print'

logs = `git reflog`.scan(/moving from (\S+) to (\S+)/).flatten.uniq

if ARGV.first
  puts logs.grep(Regexp.new(ARGV.first))
else
  puts logs.first(30)
end

# puts logs.first(20)

# ap scanned: logs.scan(/moving from (\S+) to (\S+)/).flatten.uniq

__END__
spike/null-out-model
__END_OUTPUT__



